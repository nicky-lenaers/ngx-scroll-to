{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@nicky-lenaers/ngx-scroll-to": [
        "dist/ngx-scroll-to"
      ],
      "nicky-lenaers/ngx-scroll-to/*": [
        "dist/ngx-scroll-to/*"
      ]
    }
  }
}
