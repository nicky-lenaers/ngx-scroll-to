    "lib:publish": "cd ./dist/ngx-scroll-to && npm publish --access=public",
    "lib:test": "ng test ngx-scroll-to --watch=false",
    "lib:test:watch": "ng test ngx-scroll-to --watch=true",
    "start": "ng serve",
    "tags": "npm dist-tag ls @nicky-lenaers/ngx-scroll-to",
